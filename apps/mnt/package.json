{
  "private": true,
  "name": "mnt",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 3001",
    "lint": "eslint . --ignore-path ../../.gitignore",
    "lint-staged": "lint-staged",
    "sort-deps": "prettier-package-json --write ./package.json",
    "start": "next start",
    "test": "jest"
  },
  "dependencies": {
    "@healthline/components": "~1.0.0",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@healthline/eslint-config": "~0.0.1",
    "eslint": "~7.26.0",
    "eslint-plugin-react": "~7.23.2",
    "eslint-plugin-jest": "~24.3.6",
    "jest": "~26.6.3",
    "prettier-package-json": "~2.5.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "lint-staged": "~11.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint . --ignore-path ../../.gitignore --fix",
      "pretty-quick --write",
      "sort-deps",
      "git add .",
      "jest --bail --findRelatedTests"
    ]
  }
}
